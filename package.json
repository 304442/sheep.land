  "description": "Build optimization for sheep.land website",
  "scripts": {
    "build": "node tools/build/optimize.js",
    "build:prod": "node tools/build/optimize.js --production",
    "minify": "node tools/build/optimize.js",
    "clean": "rm -rf dist",
    "help": "node tools/build/optimize.js --help",
